{
  "name": "abstrax",
  "version": "0.4.0",
  "description": "A simple wrapper on top of jQuery.ajax to reduce redundancy",
  "main": "lib/abstrax.js",
  "files": [
    "lib",
    "README.md",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:lib",
    "build:lib": "babel src -d --out-dir lib --ignore '**/*.test.js'",
    "clean": "rimraf lib",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint --fix src",
    "prepublish": "npm run build",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farneman/abstrax.git"
  },
  "homepage": "https://github.com/farneman/abstrax",
  "bugs": {
    "url": "https://github.com/farneamn/abstrax/issues"
  },
  "keywords": [
    "jQuery",
    "ajax",
    "request"
  ],
  "author": "Josh Farneman <josh@farneman.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "codeclimate-test-reporter": "^0.5.1",
    "core-js": "^3.0.0",
    "eslint": "^7.2.0",
    "flow-bin": "^0.127.0",
    "jest": "^26.0.1"
    "jquery": "^3.5.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "jquery": "*"
  }
}
